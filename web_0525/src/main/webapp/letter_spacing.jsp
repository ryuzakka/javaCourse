<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
	<meta charset="UTF-8">
	<title>letter spacing</title>
	<style>
		div {
			letter-spacing:2px;
		}
		#a {
			letter-spacing:4px;
		}
		#aa {
			letter-spacing:8px;
		}
		#b {
			word-spacing:10px;
		}
		#bb {
			word-spacing:20px;
		}
	</style>
</head>
<body>
	<!-- letter-spacing:ê°’ -->
	
	<div> Lorem ipsum dolor sit amet </div> <p>
	<div id="a"> Lorem ipsum dolor sit amet </div> <p>
	<div id="aa"> Lorem ipsum dolor sit amet </div> <p>
	<div id="b"> Lorem ipsum dolor sit amet </div> <p>
	<div id="bb"> Lorem ipsum dolor sit amet </div> <p>
	<hr>
	
</body>
</html>